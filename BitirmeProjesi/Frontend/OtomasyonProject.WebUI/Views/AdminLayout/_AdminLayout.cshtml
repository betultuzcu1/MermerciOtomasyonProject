
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Html.PartialAsync("/Views/AdminLayout/HeadPartial.cshtml")


<body>
    @await Html.PartialAsync("/Views/AdminLayout/PreleaderPartial.cshtml")
    <div id="main-wrapper">
        @await Html.PartialAsync("/Views/AdminLayout/NavheaderPartial.cshtml")
        <!--Header-->
        @await Html.PartialAsync("/Views/AdminLayout/HeaderPartial.cshtml")
        <!--Sidebar-->
        @await Html.PartialAsync("/Views/AdminLayout/SidebarPartial.cshtml")
        <!--Content body-->
        <div class="content-body">

            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                               @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- #/ container -->
        </div>
        <!--Footer-->
        @await Html.PartialAsync("/Views/AdminLayout/FooterPartial.cshtml")
        
    </div>
    <!--Scripts-->
    <script src="~/adminweb/plugins/common/common.min.js"></script>
    <script src="~/adminweb/js/custom.min.js"></script>
    <script src="~/adminweb/js/settings.js"></script>
    <script src="~/adminweb/js/gleek.js"></script>
    <script src="~/adminweb/js/styleSwitcher.js"></script>

</body>

</html>